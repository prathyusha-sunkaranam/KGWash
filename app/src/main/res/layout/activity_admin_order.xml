            <TextView
            android:layout_weight="0.4"

            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:text="Mobile Number :"
            android:inputType="text" />


            <TextView


            android:layout_weight="0.4"
            android:id="@+id/mobile_admin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="text" />
                <ImageView
                    android:id="@+id/phcall"
