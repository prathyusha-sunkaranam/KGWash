                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/phcall"/>



            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:weightSum="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="0.4"

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:text=" Email :"
                    android:inputType="text" />

                <TextView

                    android:layout_weight="0.4"
                    android:id="@+id/email_admin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:inputType="text" />
                <ImageView
                    android:layout_weight="0.2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/emailadmin"/>



            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:weightSum="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="0.4"

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:text=" Adress :"
                    android:inputType="text" />

                <TextView


                    android:layout_weight="0.4"
                    android:id="@+id/adress_admin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:inputType="text" />
                <ImageView
                    android:layout_weight="0.2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:src="@drawable/mapsadmin"/>




            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:weightSum="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="0.5"

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:text=" LandMark :"
                    android:inputType="text" />

                <TextView

                    android:layout_weight="0.5"
                    android:id="@+id/landmark_admin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:inputType="text" />


            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:weightSum="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView

                    android:layout_weight="0.5"
                    android:id="@+id/date_admin"

                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:text=" Date :"
                    android:inputType="text" />

                <TextView
                    android:text="TimeSlot"

                    android:layout_weight="0.5"
                    android:id="@+id/timeslot_admin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:inputType="text" />


            </LinearLayout>
        </LinearLayout>








